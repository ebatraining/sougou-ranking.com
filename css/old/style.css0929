}
.three_flow_inner{
width: 292px;
float: left;
margin-right: 20px;
}
.three_flow_inner img{
width: 100%;
height: auto;
}
.three_flow_inner:last-child{
margin-right: 0;
}
/*overlayer
--------------------------------------*/
.overlay{
width: 0;
height: 0;
opacity: 0;
background: rgba(0,0,0,0.6);
position: fixed;
transition:opacity 0.3s ease-in;
-webkit-transition:opacity 0.3s ease-in;
-moz-transition:opacity 0.3s ease-in;
-o-transition:opacity 0.3s ease-in;
}
.overlay:target{
width: 100%;
height: 100%;
opacity: 1;
z-index: 9999;
top: 0;
left: 0;
}
.overlay ul{
width: 0;
height: 0;
opacity: 0;
}
.overlay:target ul{
width: 1020px;
height: 550px;
background-color: #fff;
border-radius: 10px;
box-shadow: 0px 0px 3px rgba(0,0,0,0.6);
margin: 40px auto;
opacity: 1;
}
.yoko-baner img{
width: 980px;
height: 100px;
}
/*site-map
----------------------------------------------------*/
.site_map {
margin-top: 120px;
}
.site_map h2 {
color: #444;
font-size: 0.875em;
font-weight: bold;
}
.site_map ul {
width: 49%;
float: left;
margin-right: 1%;
}
.site_map ul li{
line-height: 40px;
}
.site_map ul li a{
display: block;
border-bottom: 2px dotted #aaa;
font-size: 0.75em;
color: #0078ff;
}
.site_map ul li a:hover{
color: #00f;
}
.site_map ul li ul{
list-style-type: disc;
padding-left: 40px;
}
.site_map ul li ul li a{
border: none;
}
#site_map_navigetion{
position: absolute;
right: 0px;
top: 0;
height: 40px;
}
#site_map_navigetion a{
color: #0078ff;
display: inline-block;
line-height: 40px;
font-weight: bold;
}
#site_map_navigetion a:hover{
color: #0000ff;
}
/*alert
----------------------------------------------------*/
h2.alert {
color: #dd0000;
font-size: 1em;
line-height: 40px;
}
.site_map h3,.site_map h4{
font-size: 0.875em;
line-height: 1.6em;
color: #444;
}
.site_map h4 a{
color: #0078ff;
}
.site_map h4 a:hover{
color: #0000ff;
}
/*clearfix
----------------------------------------------------*/
.clearfix{
zoom:1;
}
.clearfix:after{
content:"";
display:block;
clear:both;
}
